# Telegram Bot Configuration
BOT_TOKEN=your_telegram_bot_token_here

# Supabase Configuration
SUPABASE_URL=
SUPABASE_KEY=

# Optional: Comma-separated list of allowed Telegram user IDs
# Leave empty to allow all users
ALLOWED_USERS=

# Rate Limiting Configuration
# Progress update interval in seconds (default: 2.0)
PROGRESS_UPDATE_INTERVAL=2.0

# Update progress every N records (default: 10)
PROGRESS_UPDATE_BATCH_SIZE=10

# Processing Configuration
# Maximum file size in MB (default: 10)
MAX_FILE_SIZE_MB=10

# Number of records to process in each batch (default: 50)
BATCH_SIZE=50

# Delay between batches in seconds (default: 0.1)
BATCH_DELAY=0.1

# Logging
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
LOG_LEVEL=INFO

# Paths
# Directory for university logos CSV files (default: logos)
LOGOS_DIR=logos
